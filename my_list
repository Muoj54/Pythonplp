my_list = []
my_list.append(10)
my_list.append(20)
my_list.append(30)
my_list.append(40)
my_list.insert(1, 15)#Insert elementsat second position
my_list.extend([50, 60, 70])#Additional elements
last_element = my_list.pop() #Remove the last element
my_list.sort()#sort in ascending order
index_of_30 = my_list.index(30)#find index of the value 30
print("Updated list:", my_list)
print("Removed last element:", last_element)
print("Index of value 30:", index_of_30)
